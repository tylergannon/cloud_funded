- if content_for?(:open_graph)
  = yield :open_graph
- else
  %link{rel: 'image_src', href: '/assets/logo.png'}
  %meta{property: 'fb:app_id', content: facebook_app_id}
  %meta{property: 'og:type', content: 'website'}
  %meta{property: 'og:title', content: 'CloudFunded'}
